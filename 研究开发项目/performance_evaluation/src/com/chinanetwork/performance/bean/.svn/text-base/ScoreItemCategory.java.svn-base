package com.chinanetwork.performance.bean;

public class ScoreItemCategory {

	private int departmentScoreCategoryId;
	private String departmentScoreCategoryName;
	
	
	
	public void setDepartmentScoreCategoryName(String departmentScoreCategoryName) {
		this.departmentScoreCategoryName = departmentScoreCategoryName;
	}
	public String getDepartmentScoreCategoryName() {
		return departmentScoreCategoryName;
	}
	public void setDepartmentScoreCategoryId(int departmentScoreCategoryId) {
		this.departmentScoreCategoryId = departmentScoreCategoryId;
	}
	public int getDepartmentScoreCategoryId() {
		return departmentScoreCategoryId;
	}
	
	
	public ScoreItemCategory(){}
	
	public ScoreItemCategory( int departmentScoreCategoryId, String departmentScoreCategoryName )
	{
		this.departmentScoreCategoryId = departmentScoreCategoryId;
		this.departmentScoreCategoryName = departmentScoreCategoryName;
		
	}
	
}
