package com.chinanetwork.performance.bean;

import java.util.Date;

public class DepartmentScoreRecord {
	
	
	private int id;
	private Department departmentFrom;
	private Department departmentTo;
	private DepartmentScoreItem departmentScoreItem;
	private float score;
	private String scoreComment;
	private Date date;
	private int whichYear;
	private int whichMonth;
	private int modifyTimes;
	

	public int getModifyTimes() {
		return modifyTimes;
	}
	public void setModifyTimes(int modifyTimes) {
		this.modifyTimes = modifyTimes;
	}
	public Department getDepartmentFrom() {
		return departmentFrom;
	}
	public void setDepartmentFrom(Department departmentFrom) {
		this.departmentFrom = departmentFrom;
	}
	public Department getDepartmentTo() {
		return departmentTo;
	}
	public void setDepartmentTo(Department departmentTo) {
		this.departmentTo = departmentTo;
	}
	
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}

	public DepartmentScoreItem getDepartmentScoreItem() {
		return departmentScoreItem;
	}
	public void setDepartmentScoreItem(DepartmentScoreItem departmentScoreItem) {
		this.departmentScoreItem = departmentScoreItem;
	}
	public float getScore() {
		return score;
	}
	public void setScore(float score) {
		this.score = score;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	
	public String getScoreComment() {
		return scoreComment;
	}
	public void setScoreComment(String scoreComment) {
		this.scoreComment = scoreComment;
	}
	public int getWhichYear() {
		return whichYear;
	}
	public void setWhichYear(int whichYear) {
		this.whichYear = whichYear;
	}
	public int getWhichMonth() {
		return whichMonth;
	}
	public void setWhichMonth(int whichMonth) {
		this.whichMonth = whichMonth;
	}
	public DepartmentScoreRecord(int id, Department departmentFrom,
			Department departmentTo, DepartmentScoreItem departmentScoreItem,
			float score, String scoreComment, Date date, int whichYear,
			int whichMonth, int modifyTimes) {
		super();
		this.id = id;
		this.departmentFrom = departmentFrom;
		this.departmentTo = departmentTo;
		this.departmentScoreItem = departmentScoreItem;
		this.score = score;
		this.scoreComment = scoreComment;
		this.date = date;
		this.whichYear = whichYear;
		this.whichMonth = whichMonth;
		this.modifyTimes = modifyTimes;
	}
	public DepartmentScoreRecord() {
		super();
	}

	
	
	
	

}
