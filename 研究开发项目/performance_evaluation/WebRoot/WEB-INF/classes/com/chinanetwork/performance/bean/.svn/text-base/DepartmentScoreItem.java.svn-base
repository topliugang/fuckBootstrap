package com.chinanetwork.performance.bean;

import java.util.Date;

public class DepartmentScoreItem {
	private int departmentScoredItemId;
	private String departmentScoreName;
	private String scoreDescription;
	private float scoreValue;
	private String scoreReference;
	private String scoreCalculationMethod;
	private Department department;
	private ScoreItemCategory scoreItemCategory;
	private Date scoreDate;
	private int scoreInUse;

	// private List<Department> dataScourceDepartments;

	public String getDepartmentScoreName() {
		return departmentScoreName;
	}

	public void setDepartmentScoreName(String departmentScoreName) {
		this.departmentScoreName = departmentScoreName;
	}

	public String getScoreCalculationMethod() {
		return scoreCalculationMethod;
	}

	public void setScoreCalculationMethod(String scoreCalculationMethod) {
		this.scoreCalculationMethod = scoreCalculationMethod;
	}

	public int getDepartmentScoredItemId() {
		return departmentScoredItemId;
	}

	public void setDepartmentScoredItemId(int departmentScoredItemId) {
		this.departmentScoredItemId = departmentScoredItemId;
	}

	public String getScoreDescription() {
		return scoreDescription;
	}

	public void setScoreDescription(String scoreDescription) {
		this.scoreDescription = scoreDescription;
	}

	public float getScoreValue() {
		return scoreValue;
	}

	public void setScoreValue(float scoreValue) {
		this.scoreValue = scoreValue;
	}

	public String getScoreReference() {
		return scoreReference;
	}

	public void setScoreReference(String scoreReference) {
		this.scoreReference = scoreReference;
	}

	public ScoreItemCategory getScoreItemCategory() {
		return scoreItemCategory;
	}

	public void setScoreItemCategory(ScoreItemCategory scoreItemCategory) {
		this.scoreItemCategory = scoreItemCategory;
	}

	public Date getScoreDate() {
		return scoreDate;
	}

	public void setScoreDate(Date scoreDate) {
		this.scoreDate = scoreDate;
	}

	public int getScoreInUse() {
		return scoreInUse;
	}

	public void setScoreInUse(int scoreInUse) {
		this.scoreInUse = scoreInUse;
	}

	// public void setDataScourceDepartments(List<Department>
	// dataScourceDepartments) {
	// this.dataScourceDepartments = dataScourceDepartments;
	// }
	// public List<Department> getDataScourceDepartments() {
	// return dataScourceDepartments;
	// }

	public Department getDepartment() {
		return department;
	}

	public DepartmentScoreItem() {
		super();
	}

	public DepartmentScoreItem(int departmentScoredItemId,
			String departmentScoreName, String scoreDescription,
			float scoreValue, String scoreReference,
			String scoreCalculationMethod, Department department,
			ScoreItemCategory scoreItemCategory, Date scoreDate, int scoreInUse) {
		super();
		this.departmentScoredItemId = departmentScoredItemId;
		this.departmentScoreName = departmentScoreName;
		this.scoreDescription = scoreDescription;
		this.scoreValue = scoreValue;
		this.scoreReference = scoreReference;
		this.scoreCalculationMethod = scoreCalculationMethod;
		this.department = department;
		this.scoreItemCategory = scoreItemCategory;
		this.scoreDate = scoreDate;
		this.scoreInUse = scoreInUse;
	}

	public void setDepartment(Department department) {
		this.department = department;
	}

}
